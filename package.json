{
  "name": "example",
  "description": "Example of the Tapjaw implementation",
  "version": "0.1.0",
  "author": "Jeramy Wenserit",
  "bin": {
    "example": "./bin/run"
  },
  "bugs": "https://github.com/digidip/example/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "dotenv": "^8.0.0",
    "restify": "^8.4.0",
    "restify-errors": "^8.0.1",
    "tapjaw-importer": "https://github.com/digidip/tapjaw-importer.git#semver:^0.2.0",
    "tslib": "^1",
    "typescript": "^3.7"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/tslint": "^3",
    "@types/node": "^10",
    "@types/restify": "^8.4.0",
    "@types/restify-errors": "^4.3.3",
    "globby": "^10",
    "ts-node": "^8.4.1",
    "tslint": "^5"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/digidip/example",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "example",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "digidip/example",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "version": "oclif-dev readme && git add README.md",
    "server": "node_modules/ts-node/dist/bin.js api/example-api.ts"
  },
  "types": "lib/index.d.ts"
}
